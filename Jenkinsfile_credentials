pipeline{
  agent any
  environment{
    GITHUB_CREDS=credentials('git_priv')
    AGENT_creds=credentials('ssh_keys')
  }
  stages{
    stage('print_creds'){
      echo "value of my original USERNAME:PASSWORD:$(GITHUB_CREDS)"
      echo "Github user: $(GITHUB_CREDS_USR)"
      echo "Githubtoken: $(GITHUB_CREDS_PSW)"
    }
  }
}
